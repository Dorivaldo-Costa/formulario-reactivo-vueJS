" @reset=\"resetar\">\n\n            <div class=\"form-group\">\n              <label>Nome:</label>\n              <input \n                type=\"text\" \n                class=\"form-control\" \n                placeholder=\"Seu nome\"\n                v-model.lazy.trim=\"desenvolvedor.nome\">\n            </div>\n\n            <div class=\"form-group\">\n              <label>Endereço de email:</label>\n              <input \n                type=\"email\" \n                class=\"form-control\" \n                placeholder=\"Seu email\"\n                :value=\"desenvolvedor.email\"\n                @input=\"desenvolvedor.email = $event.target.value\">\n            </div>\n\n            <div class=\"form-group\">\n              <label>Idade:</label>\n              <input \n                type=\"number\" \n                class=\"form-control\" \n                placeholder=\"Sua idade\"\n                v-model.number=\"desenvolvedor.idade\">\n            </div>\n\n            <div class=\"form-group\">\n\n              <p>Gênero:</p>\n\n              <div class=\"form-check form-check-inline\">\n                <input \n                  type=\"radio\" \n                  class=\"form-check-input\" \n                  value=\"Masculino\"\n                  v-model=\"desenvolvedor.genero\">\n                <label class=\"form-check-label\">Masculino</label>\n              </div>\n\n              <div class=\"form-check form-check-inline\">\n                <input \n                  type=\"radio\" \n                  class=\"form-check-input\" \n                  value=\"Feminino\"\n                  v-model=\"desenvolvedor.genero\">\n                <label class=\"form-check-label\">Feminino</label>\n              </div>\n\n            </div>\n\n            <div class=\"form-group\">\n              <label>Ocupação:</label>\n              <select class=\"form-control\" v-model=\"desenvolvedor.ocupacao\">\n                <option value=\"\" disabled>Selecione